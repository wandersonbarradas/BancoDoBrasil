{
    "name": "seuvendor/banco-do-brasil",
    "description": "Integração com a API de Cobrança do Banco do Brasil",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Seu Nome",
            "email": "seu.email@exemplo.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/cache": "^8.0|^9.0|^10.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SeuVendor\\BancoDoBrasil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeuVendor\\BancoDoBrasil\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SeuVendor\\BancoDoBrasil\\BancoDoBrasilServiceProvider"
            ],
            "aliases": {
                "BancoDoBrasil": "SeuVendor\\BancoDoBrasil\\Facades\\BancoDoBrasil"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
